{
  "version": 3,
  "configurePresets": [
    {
      "name": "vs2022-windows",
      "generator": "Visual Studio 17 2022",
      "binaryDir": "${sourceDir}/vsbuild",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": {
          "type": "STRING",
          "value": "RelWithDebInfo"
        },
        "DEPENDENCIES_DIR": {
          "type": "PATH",
          "value": "${sourceDir}/../.."
        },
        "BOOST_ROOT": {
          "type": "PATH",
          "value": "${sourceDir}/../../boost_1_88_0"
        },
        "LZ4_ROOT": {
          "type": "PATH",
          "value": "${sourceDir}/../../lz4-v1.10.0"
        },
        "ZLIB_ROOT": {
          "type": "PATH",
          "value": "${sourceDir}/../../zlib-v1.3.1"
        },
        "QT_ROOT": {
          "type": "PATH",
          "value": "C:/Qt/6.7.3/msvc2019_64"
        },
        "PYTHON_ROOT": {
          "type": "PATH",
          "value": "${sourceDir}/../../python-3.12.10"
        }
      }
    }
  ]
}